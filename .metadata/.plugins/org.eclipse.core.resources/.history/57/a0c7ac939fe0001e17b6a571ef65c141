<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
    http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <!-- Registering bean of type HelloService with some ID: myBean -->
	
	<bean id="student" class="day.Student">
		<property name="studentId" value="101"/>
		<property name="studentName" value="Sanika"/>
	</bean>
    
    <bean id="student1" class="day.Student">
    	<constructor-arg value="101" />
    	<constructor-arg value="Sanika" />
    </bean>
    
    <bean id="student2" class="day.Student">
    	<constructor-arg value="102" />
    	<constructor-arg value="Akshay" />
    </bean>
</beans>